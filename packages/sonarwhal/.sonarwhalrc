{
    "connector": {
        "name": "chrome",
        "options": {
            "waitFor": 1000
        }
    },
    "formatters": ["summary", "excel"],
    "parsers": ["javascript"],
    "rulesTimeout": 120000,
    "rules": {

        "no-bom": "warning",
        "no-broken-links": "warning",
        "no-disallowed-headers": "warning",
        "no-http-redirects": "warning"
    }
}
