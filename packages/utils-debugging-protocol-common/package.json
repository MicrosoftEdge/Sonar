    "@typescript-eslint/eslint-plugin": "^1.4.2",
    "@typescript-eslint/parser": "1.4.2",
    "chrome-remote-debug-protocol": "^1.2.20180422",
    "cpx": "^1.5.0",
    "eslint": "^5.15.1",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-markdown": "^1.0.0",
    "hint": "^4.5.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^2.6.3",
    "typescript": "^3.3.3333"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "dist"
  ],
  "homepage": "https://webhint.io/",
  "keywords": [
    "connector",
