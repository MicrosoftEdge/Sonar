{
  "ava": {
    "failFast": false,
    "files": [
      "dist/tests/**/*.js"
    ],
    "timeout": "1m"
  },
  "dependencies": {
    "js-library-detector": "^5.1.0",
    "lodash": "^4.17.11",
    "semver": "^5.5.1"
  },
  "description": "hint that that checks using Snyk for vulnerable JavaScript libraries",
  "devDependencies": {
    "@hint/utils-tests-helpers": "^1.0.1",
    "angular": "1.4.9",
    "ava": "^0.25.0",
    "cpx": "^1.5.0",
    "eslint": "^5.6.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-markdown": "^1.0.0-beta.7",
    "eslint-plugin-typescript": "^0.12.0",
    "hint": "^3.4.8",
    "jquery": "2.1.4",
    "knockout": "https://github.com/knockout/knockout/archive/v3.4.0-rc.tar.gz",
    "markdownlint-cli": "^0.13.0",
    "moment": "2.22.2",
    "npm-link-check": "^2.0.0",
    "npm-run-all": "^4.1.2",
    "nyc": "^13.0.1",
    "rimraf": "^2.6.2",
    "typescript": "^3.0.3",
    "typescript-eslint-parser": "^20.0.0"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "dist"
  ],
  "greenkeeper": {
    "ignore": [
      "angular",
      "jquery"
    ]
  },
  "homepage": "https://webhint.io/",
  "keywords": [
    "no-vulnerable-javascript-libraries",
    "no-vulnerable-javascript-libraries-hint",
    "webhint",
    "webhint-hint",
    "webhint-recommended"
  ],
  "license": "Apache-2.0",
  "main": "dist/src/hint.js",
  "name": "@hint/hint-no-vulnerable-javascript-libraries",
  "nyc": {
    "extends": "../../.nycrc"
  },
  "peerDependencies": {
    "hint": "^3.4.8"
  },
  "repository": "webhintio/hint",
  "scripts": {
    "build": "npm run clean && npm-run-all build:*",
    "build-release": "npm run clean && npm run build:assets && tsc --inlineSourceMap false --removeComments true",
    "build:assets": "cpx \"./{src,tests}/**/{!(*.ts),.!(ts)}\" dist",
    "build:ts": "tsc",
    "clean": "rimraf dist",
    "lint": "npm-run-all lint:*",
    "lint:js": "eslint . --cache --ext js --ext md --ext ts --ignore-path ../../.eslintignore --report-unused-disable-directives",
    "lint:md": "markdownlint --ignore CHANGELOG.md *.md",
    "test": "npm run lint && npm run build && npm run test-only",
    "test-only": "nyc ava",
    "watch": "npm run build && npm-run-all --parallel -c watch:*",
    "watch:assets": "npm run build:assets -- -w --no-initial",
    "watch:test": "ava --watch",
    "watch:ts": "npm run build:ts -- --watch"
  },
  "version": "1.6.0"
}
