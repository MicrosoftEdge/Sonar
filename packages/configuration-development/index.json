{
    "connector": "local",
    "formatters": [
        "html",
        "summary"
    ],
    "hints": {
        "axe": "error",
        "babel-config/is-valid": "error",
        "disown-opener": "error",
        "highest-available-document-mode": "error",
        "meta-charset-utf-8": "error",
        "meta-viewport": "error",
        "no-bom": "error",
        "no-protocol-relative-urls": "error",
        "sri": "error",
        "typescript-config/consistent-casing": "error",
