{
  "ava": {
    "failFast": false,
    "files": [
      "dist/tests/**/*.js"
    ],
    "timeout": "1m"
  },
  "description": "webhint parser needed to analyze webpack config files",
  "devDependencies": {
    "@types/webpack": "^4.4.19",
    "ava": "^0.25.0",
    "cpx": "^1.5.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-markdown": "^1.0.0-rc.0",
    "eslint-plugin-typescript": "0.14.0",
    "hint": "^4.1.0",
    "npm-link-check": "^3.0.0",
    "npm-run-all": "^4.1.5",
    "nyc": "^13.1.0",
    "proxyquire": "2.0.0",
    "rimraf": "^2.6.2",
    "sinon": "^7.1.1",
    "typescript": "^3.1.6",
    "typescript-eslint-parser": "21.0.2"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "dist/src"
  ],
  "homepage": "https://webhint.io/",
  "keywords": [
    "webhint",
    "webhint-parser",
    "webpack-config"
  ],
  "license": "Apache-2.0",
  "main": "dist/src/parser.js",
  "name": "@hint/parser-webpack-config",
  "nyc": {
    "extends": "../../.nycrc"
  },
  "peerDependencies": {
    "hint": "^4.1.0"
  },
  "repository": "webhintio/hint",
  "scripts": {
    "build": "npm run clean && npm-run-all build:*",
    "build:assets": "cpx \"./{src,tests}/**/{!(*.ts),.!(ts)}\" dist",
    "build:ts": "tsc -b",
    "build-release": "npm run clean && npm run build:assets && tsc --inlineSourceMap false --removeComments true",
    "clean": "rimraf dist",
    "lint": "npm-run-all lint:*",
    "lint:js": "eslint . --cache --ext js --ext md --ext ts --ignore-path ../../.eslintignore --report-unused-disable-directives",
    "lint:md": "node ../../scripts/lint-markdown.js",
    "test": "npm run lint && npm run build && npm run test-only",
    "test-only": "nyc ava",
    "test-release": "npm run lint && npm run clean && npm run build:assets && tsc && npm run test-only",
    "watch": "npm run build && npm-run-all --parallel -c watch:*",
    "watch:assets": "npm run build:assets -- -w --no-initial",
    "watch:test": "ava --watch",
    "watch:ts": "npm run build:ts -- --watch"
  },
  "version": "2.0.1"
}
